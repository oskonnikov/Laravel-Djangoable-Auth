{
  "name": "oskonnikov/laravel-djangoable-auth",
  "description": "An authentication driver for Laravel5 which allows to authenticate users using their legacy Django PBKDF2, forked version",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Oleg Skonnikov",
      "email": "admin@xdevops.ru"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/database": "5.8.*",
    "illuminate/auth" : "5.8.*",
    "illuminate/contracts" : "5.8.*",
    "illuminate/hashing" : "5.8.*"
  },
  "require-dev" : {
    "mockery/mockery": "~0.9.1",
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Jobinja\\Djangoable\\": "src/"
    }
  }
}
